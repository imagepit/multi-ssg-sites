    "@types/mdx": "^2.0.13",
    "@types/node": "24.5.2",
    "@types/react": "^19.1.14",
    "@types/react-dom": "^19.1.9",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "gray-matter": "^4.0.3",
    "mdast": "^3.0.0",
    "micromatch": "^4.0.7",
