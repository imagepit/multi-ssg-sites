    "@orama/orama": "^3.1.14",
    "clsx": "^2.1.1",
    "fast-glob": "^3.3.2",
    "fumadocs-core": "16.4.8",
    "fumadocs-mdx": "14.2.6",
    "fumadocs-ui": "16.4.8",
    "lucide-react": "^0.544.0",
    "mermaid": "^11.12.0",
    "next": "16.1.0",
    "next-image-export-optimizer": "^1.19.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
